<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/base_nav_graph">
    <fragment
        android:id="@+id/nav_settings"
        android:name="com.example.somoni.presenetation.uisetting.FlowSettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_flow_settings" />
    <fragment
        android:id="@+id/nav_main"
        android:name="com.example.somoni.presenetation.uimain.FlowMainFragment"
        android:label="Main"
        tools:layout="@layout/flow_main_fragment" />
    <action
        android:id="@+id/action_global_to_nav_setings"
        app:destination="@id/nav_settings"
        app:popUpTo="@id/base_nav_graph" />
    <action
        android:id="@+id/action_global_to_nav_main"
        app:destination="@id/nav_main"
        app:popUpTo="@id/base_nav_graph" />
</navigation>